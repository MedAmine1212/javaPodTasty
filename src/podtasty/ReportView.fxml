<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="108.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="podtasty.ReportView">
   <children>
      <Label layoutX="33.0" layoutY="40.0" prefHeight="18.0" prefWidth="83.0" text="Report ID" textFill="WHITE" />
      <Label layoutX="216.0" layoutY="40.0" text="Type" textFill="WHITE" />
      <Label layoutX="33.0" layoutY="67.0" text="Description" textFill="WHITE" />
      <Label fx:id="id" layoutX="150.0" layoutY="39.0" styleClass="id" text="id" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label fx:id="type" layoutX="258.0" layoutY="40.0" prefHeight="18.0" prefWidth="317.0" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label fx:id="description" layoutX="150.0" layoutY="64.0" prefHeight="18.0" prefWidth="203.0" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Button fx:id="accept" layoutX="417.0" layoutY="69.0" mnemonicParsing="false" onMouseClicked="#accept" text="Accept" />
      <Button fx:id="refuse" layoutX="478.0" layoutY="69.0" mnemonicParsing="false" onMouseClicked="#refuse" text="Refuse" />
      <Label layoutX="33.0" layoutY="14.0" text="Podcast name" textFill="WHITE" />
      <Label fx:id="name" layoutX="150.0" layoutY="14.0" textFill="white">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label fx:id="state" layoutX="550.0" layoutY="75.0" textFill="#ff4848">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="blockbtn" layoutX="310.0" layoutY="69.0" mnemonicParsing="false" text="Block podcast" onMouseClicked="#blockpost"/>
   </children>
</AnchorPane>
